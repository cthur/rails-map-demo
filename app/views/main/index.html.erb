<h1>Google Map</h1>

<div id="map"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: -34.397, lng: 150.644},
      zoom: 8
    });
  }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD2HyLFO9h8J0Yp0SAUWKBSiNKkWfcGRDg&callback=initMap"
async defer></script>
